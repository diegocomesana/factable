{
  "name": "factable",
  "version": "1.0.0-alpha.1",
  "description": "A test case generation and managment tool for nodejs babel transpiled projects",
  "main": "./factable/index.js",
  "scripts": {
    "clean": "./clean.sh",
    "plugin:test": "cross-env FACTABLE_TRANSPILE=on jest ./factable/plugin/tests",
    "plugin:dev": "cross-env FACTABLE_TRANSPILE=on nodemon ./factable/plugin/src/dev",
    "plugin:dev-build": "cross-env FACTABLE_TRANSPILE=on babel -w -d ./factable/plugin/build-test ./factable/plugin/tests/fixtures/full-on",
    "client:build": "cross-env-shell webpack --config ./factable/client/webpack.config.js",
    "server:build": "cross-env-shell babel -d ./factable/build/server ./factable/server",
    "build": "cross-env-shell npm run client:build && npm run server:build",
    "server:run": "cross-env-shell node ./factable/build/server",
    "boot": "npm run clean && cross-env NODE_ENV=production npm run build && cross-env NODE_ENV=production npm run server:run",
    "client:watch": "webpack --config ./factable/client/webpack.config.js --watch",
    "server:watch": "nodemon --watch ./factable/server --exec babel-node ./factable/server",
    "dev": "npm run clean && npm run client:watch & npm run server:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diegocomesana/factable.git"
  },
  "keywords": [
    "test",
    "case",
    "generation",
    "management",
    "tool",
    "nodejs"
  ],
  "author": "Diego Manuel Comesa√±a",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/diegocomesana/factable/issues"
  },
  "homepage": "https://github.com/diegocomesana/factable#readme",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/generator": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/parser": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/template": "^7.10.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-plugin-tester": "^9.2.0",
    "cardinal": "^2.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "jest": "^26.4.2",
    "mini-css-extract-plugin": "^0.12.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "sass-loader": "^10.0.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-node-externals": "^2.5.2",
    "ws": "^7.3.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "open": "^7.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.2.0",
    "styled-normalize": "^8.0.7"
  }
}