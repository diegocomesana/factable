{
  "name": "factable",
  "version": "1.0.0-alpha.3",
  "description": "A test case generation and managment tool for nodejs babel transpiled projects",
  "main": "./src/index.js",
  "scripts": {
    "clean": "./clean.sh",
    "plugin:test": "cross-env FACTABLE_TRANSPILE=6789 jest ./src/plugin/tests",
    "plugin:dev": "cross-env FACTABLE_TRANSPILE=6789 nodemon ./src/plugin/dev.js",
    "plugin:dev-build": "cross-env FACTABLE_TRANSPILE=6789 babel -w -d ./build-tests ./src/plugin/tests/fixtures/full-on",
    "client:build": "cross-env-shell webpack --config ./src/client/webpack.config.js",
    "server:build": "cross-env-shell babel -d ./build/server ./src/server",
    "plugin:build": "cross-env-shell babel -d ./build/plugin ./src/plugin --ignore ./src/plugin/dev.js --ignore ./src/plugin/tests",
    "evidencer:build": "cross-env-shell babel -d ./build/evidencer ./src/evidencer",
    "build": "cross-env-shell npm run clean && npm run client:build && npm run server:build && npm run plugin:build && npm run evidencer:build",
    "server:run": "cross-env-shell node ./build/server/run.js",
    "boot": "cross-env NODE_ENV=production npm run build && cross-env NODE_ENV=production npm run server:run",
    "client:watch": "webpack --config ./src/client/webpack.config.js --watch",
    "server:watch": "cross-env FACTABLE_DEV=true nodemon --watch ./src/server --exec babel-node ./src/server/run.js 6789",
    "dev": "npm run client:watch & npm run server:watch"
  },
  "bin": {
    "factable-server-run": "./build/server/run.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diegocomesana/factable.git"
  },
  "keywords": [
    "test",
    "case",
    "generation",
    "management",
    "tool",
    "nodejs"
  ],
  "author": "Diego Manuel Comesa√±a",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/diegocomesana/factable/issues"
  },
  "homepage": "https://github.com/diegocomesana/factable#readme",
  "devDependencies": {},
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/generator": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/parser": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/template": "^7.10.4",
    "babel-jest": "^26.5.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-polyfill": "^6.26.0",
    "cardinal": "^2.1.1",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "jest": "^26.4.2",
    "mini-css-extract-plugin": "^0.12.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "open": "^7.3.0",
    "prettier": "^2.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass-loader": "^10.0.2",
    "simple-hashtable": "^1.0.0",
    "styled-components": "^5.2.0",
    "styled-normalize": "^8.0.7",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-node-externals": "^2.5.2",
    "ws": "^7.3.1"
  }
}
